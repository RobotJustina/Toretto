<launch>
	<arg name="K_dist"       default="0.4"/>
	<arg name="K_angle"      default="30.0"/>
	<arg name="K_brake"      default="1.0"/>
	<arg name="max_speed"    default="800"/>
	<arg name="turn_speed"   default="300"/>
	<arg name="dist_to_lane" default="100"/>
	<param name="K_dist" value="$(arg K_dist)"/>
	<param name="K_angle" value="$(arg K_angle)"/>
	<param name="K_brake" value="$(arg K_brake)"/>
	<param name="max_speed" value="$(arg max_speed)"/>
	<param name="turn_speed" value="$(arg turn_speed)"/>
	<param name="dist_to_lane" value="$(arg dist_to_lane)"/>
	<node name="lane_tracking" pkg="lane_tracking" type="lane_tracking_node" output="screen"/>
</launch>
