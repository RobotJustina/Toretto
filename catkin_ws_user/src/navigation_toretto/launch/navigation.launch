          <param name="max_gap_len" value="20" />
          <param name="value_thr_low" value="100"/>
          <param name="value_thr_high" value="255" />
          <param name="canny_thr_low" value="40" />
          <param name="canny_thr_high" value="75" />
          </node>
    <!-- <node pkg="lane_detection" type="lane_hough_node" name="lane_hough_node" output="screen">
      <remap from="image" to="/app/camera/rgb/image_raw"/> -->

      <node pkg="navigation_toretto" type="navigation_toretto" name="navigation_toretto" output="screen">
        </node>


</launch>
