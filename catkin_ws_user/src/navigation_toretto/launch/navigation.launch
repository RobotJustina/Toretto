<launch>
  <param name="vu_l1" value="1.9"/>
  <param name="vu_r1" value="2.1" />
  <param name="vu_r2" value="2.2"/>
  <param name="vu_l2" value="0.7"/>

  <param name="max_steer_left" value="10" />
  <param name="max_steer_right" value="170"/>
  <param name="Kp_nav" value="0.5"/>
  <param name="Ka_nav" value="4.0"/>
  <param name="cruise_speed" value="-220"/>




    <node pkg="laser_scan" type="laser_scan" name="laser_scan" output="log">
        </node>

        <node pkg="cross" type="cross_node" name="cross_node" output="log">
          <param name="hough_thr" value="30  "/>
