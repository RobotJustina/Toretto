<launch>
  <param name="vu_l1" value="1.9"/>
  <param name="vu_r1" value="2.1" />
  <param name="vu_r2" value="2.05"/>
  <param name="vu_l2" value="0.7"/>

  <param name="max_steer_left" value="10" />
  <param name="max_steer_right" value="170"/>

  <arg name="K_dist"       default="0.5"/>
  <arg name="K_angle"      default="30.0"/>
  <arg name="K_brake"      default="1.0"/>
  <arg name="max_speed"    default="500"/>
  <arg name="turn_speed"   default="200"/>
  <arg name="dist_to_lane" default="90"/>
  <param name="K_dist" value="$(arg K_dist)"/>
  <param name="K_angle" value="$(arg K_angle)"/>
  <param name="K_brake" value="$(arg K_brake)"/>
  <param name="max_speed" value="$(arg max_speed)"/>
  <param name="turn_speed" value="$(arg turn_speed)"/>
  <param name="dist_to_lane" value="$(arg dist_to_lane)"/>

  <node pkg="laser_scan" type="laser_scan" name="laser_scan" output="log">
      </node>

  <node pkg="cross" type="cross_node" name="cross_node" output="log">
    <param name="hough_thr" value="30  "/>
    <param name="min_lin_len" value="20" />
    <param name="max_gap_len" value="20" />
    <param name="value_thr_low" value="100"/>
    <param name="value_thr_high" value="255" />
    <param name="canny_thr_low" value="40" />
    <param name="canny_thr_high" value="75" />
    </node>
    <!-- <node pkg="lane_detection" type="lane_hough_node" name="lane_hough_node" output="screen">
      <remap from="image" to="/app/camera/rgb/image_raw"/> -->

      <node pkg="navigation_toretto" type="navigation_toretto" name="navigation_toretto" output="screen">
        </node>


</launch>
